{
  "name": "price-checker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --names \"server,client\"  \"npm run server\" \"vite\"",
    "build": "tsc --project tsconfig.build.json && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "server": "vite-node --watch ./src/server/index.ts",
    "build-app": "npm run build && cp -r src/nodejs dist/ && npx cap sync",
    "android:project": "npx cap open android",
    "android:sim": "npx cap run android --target=Pixel_6_API_31  --external",
    "android": "npx cap run android --target=62372706 --prod",
    "core": "node ./src/nodejs/index.js",
    "ionic:build": "npm run build",
    "ionic:serve": "npm run dev"
  },
  "dependencies": {
    "@capacitor-community/barcode-scanner": "^4.0.1",
    "@capacitor/android": "^5.0.0",
    "@capacitor/core": "^5.0.0",
    "@capacitor/filesystem": "^5.1.4",
    "@capacitor/haptics": "^5.0.8",
    "@fastify/cors": "^8.0.0",
    "@ionic/react": "^7.8.4",
    "@ionic/react-router": "^7.8.4",
    "@nextui-org/react": "^2.2.9",
    "@react-hook/merged-ref": "^1.3.2",
    "axios": "^1.4.0",
    "capacitor-nodejs": "https://github.com/hampoelz/capacitor-nodejs/releases/download/v1.0.0-beta.6/capacitor-nodejs.tgz",
    "dropbox": "^10.34.0",
    "fastify": "^4.15.0",
    "framer-motion": "^11.18.2",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "react": "^18.2.0",
    "react-charts": "^3.0.0-beta.57",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-query": "^3.39.3",
    "tailwind-merge": "^2.4.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/lodash": "^4.14.197",
    "@types/md5": "^2.3.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^3.0.9",
    "autoprefixer": "^10.4.15",
    "concurrently": "^8.0.1",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "jsdom": "^23.0.0",
    "mockdate": "^3.0.5",
    "postcss": "^8.4.29",
    "react-intersection-observer": "^9.5.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.5.4",
    "uuid": "^9.0.0",
    "vite": "^6.2.2",
    "vite-node": "^3.0.9",
    "vitest": "^3.0.9",
    "vitest-dom": "^0.1.1"
  }
}
